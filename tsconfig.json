{
	"compilerOptions": {
	  "target": "es2022",
	  "lib": ["dom", "dom.iterable", "es2022"],
	  "allowJs": true,
	  "esModuleInterop": true,
	  "strict": true,
	  "forceConsistentCasingInFileNames": true,
	  "module": "es2022",
	  "moduleResolution": "nodenext",
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "noEmit": false,
	  "jsx": "react-jsx",
	  "declaration": true,
	  "outDir": "dist",
	  "types": ["@emotion/react/types/css-prop"]
	},
	"include": ["src"]
  }
  